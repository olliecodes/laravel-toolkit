{
  "name": "olliecodes/laravel-toolkit",
  "description": "A toolkit for Laravel that adds additional features and functionality, that I commonly use",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Ollie Read",
      "email": "code@ollie.codes"
    }
  ],
  "require": {
    "php": "^8.0",
    "laravel/framework": "^9.0"
  },
  "require-dev": {
    "orchestra/testbench": "^7.0",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "OllieCodes\\Toolkit\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "OllieCodes\\Toolkit\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "OllieCodes\\Toolkit\\ToolkitServiceProvider"
      ],
      "aliases": [
      ]
    }
  }
}
